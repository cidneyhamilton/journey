<h1>Listing responses</h1>

<table>
  <tr>
  </tr>

<% for response in @responses %>
  <tr>
    <td><%= link_to 'Show', questionnaire_response_path(@questionnaire, response) %></td>
    <td><%= link_to 'Edit', edit_questionnaire_response_path(@questionnaire, response) %></td>
    <td><%= link_to 'Destroy', questionnaire_response_path(@questionnaire, response), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New response', new_questionnaire_response_path(@questionnaire) %>
